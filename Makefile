# Animation target for the takeoff sequence
takeoff:
	@i=1; \
	while [ $$i -le 28 ]; do \
		sleep $$(echo "scale=2; 0.3/$$i" | bc); \
		padding=$$(printf "%*s" $$i ""); \
		printf "\r🔥%slibft takes off 🚀" "$${padding// /🔥}"; \
		i=$$((i + 1)); \
	done
	@echo "\n=============================================================="

# Rule to create the library file
$(NAME): $(OBJ)
	@ar rcs $(NAME) $(OBJ)
	@echo "\033[1;37m─────────────────────────────────────────────────────────────|\033[0m"
	@echo "📁 .o count:  $(words $(OBJ))"
	@echo "\033[1;37m─────────────────────────────────────────────────────────────|\033[0m"
	@echo "┌────────────────────────────────────────────────────────────┐"
	@echo "│                                                            │"
	@echo "│    ✔️ ended                              ...libft built!    │"
	@echo "│                                                            │"
	@echo "└────────────────────────────────────────────────────────────┘"

%.o: %.c
	@start_time=$$(date +%s); \
	$(CC) $(CFLAGS) -c $< -o $@; \
	end_time=$$(date +%s); \
	printf "\033[1;30m%-20s\033[0m\t\033[1;37m%-30s\033[0m\t✔️    |\n" "Compiling:" "$<"; \
	printf "\033[1;37m%-20s\033[0m\t\033[1;37m%-30s\033[0m\t\033[1;30m[.o]\033[0m |\n" "➜ Output:" "$@"; \
	echo "\033[1;37m─────────────────────────────────────────────────────────────|\033[0m"


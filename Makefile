# Animation target for the takeoff sequence
takeoff:
	@i=1; \
	while [ $$i -le 15 ]; do \
		sleep $$(echo "scale=2; 0.5/$$i" | bc); \
		clear; \
		padding=$$(printf "%*s" $$i ""); \
		printf "üî•%slibft takes off üöÄ\n" "$${padding// /üî•}"; \
		i=$$((i + 1)); \
	done
	@echo "=================================================="

# Rule to create the library file
$(NAME): $(OBJ)
	@ar rcs $(NAME) $(OBJ)
	@echo "üìÅ .o count:  $(words $(OBJ))"
	@echo ""

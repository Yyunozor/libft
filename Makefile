# Animation target for the takeoff sequence
takeoff:
	@i=1; \
	while [ $$i -le 28 ]; do \
		sleep $$(echo "scale=2; 0.3/$$i" | bc); \
		padding=$$(printf "%*s" $$i ""); \
		printf "\r­ЪћЦ%slibft takes off ­Ъџђ" "$${padding// /­ЪћЦ}"; \
		i=$$((i + 1)); \
	done
	@echo "\n=============================================================="

